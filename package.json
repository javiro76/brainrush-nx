{
  "name": "@brainrush-nx/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev:infra:up": "docker compose -f docker-compose.dev.yml up -d",
    "dev:infra:down": "docker compose -f docker-compose.dev.yml down",
    "dev:infra:logs": "docker compose -f docker-compose.dev.yml logs -f",
    "dev:infra:restart": "npm run dev:infra:down && npm run dev:infra:up",
    "dev:auth": "npx nx serve auth-service",
    "dev:gateway": "npx nx serve api-gateway",
    "dev:content": "npx nx serve content-service",
    "dev:frontend": "npx nx serve brainrush-frontend",
    "dev:all": "concurrently \"npm run dev:infra:up\" \"npm run dev:auth\" \"npm run dev:gateway\" \"npm run dev:content\"",
    "dev:all-with-frontend": "concurrently \"npm run dev:infra:up\" \"npm run dev:auth\" \"npm run dev:gateway\" \"npm run dev:content\" \"npm run dev:frontend\"",
    "prisma:generate:auth": "npx nx prisma-generate auth-service",
    "prisma:generate:content": "npx nx prisma-generate content-service",
    "prisma:generate:exams": "npx nx prisma-generate exams-service",
    "prisma:generate:all": "npm run prisma:generate:auth && npm run prisma:generate:content && npm run prisma:generate:exams",
    "prisma:migrate:auth": "npx nx prisma-migrate-dev auth-service",
    "prisma:migrate:content": "npx nx prisma-migrate-dev content-service",
    "prisma:migrate:exams": "npx nx prisma-migrate-dev exams-service",
    "prisma:migrate:all": "npm run prisma:migrate:auth && npm run prisma:migrate:content && npm run prisma:migrate:exams",
    "prisma:studio:auth": "npx nx prisma-studio auth-service",
    "prisma:studio:content": "npx nx prisma-studio content-service",
    "prisma:studio:exams": "npx nx prisma-studio exams-service",
    "prisma:reset:auth": "npx nx prisma-migrate-reset auth-service",
    "prisma:reset:content": "npx nx prisma-migrate-reset content-service",
    "prisma:reset:exams": "npx nx prisma-migrate-reset exams-service",
    "dev:setup:auth": "npm run dev:infra:up && npm run prisma:migrate:auth && npm run prisma:generate:auth",
    "dev:setup:content": "npm run dev:infra:up && npm run prisma:migrate:content && npm run prisma:generate:content",
    "dev:setup:exams": "npm run dev:infra:up && npm run prisma:migrate:exams && npm run prisma:generate:exams",
    "dev:setup:full": "npm run dev:infra:up && npm run prisma:migrate:all && npm run prisma:generate:all",
    "dev:start:complete": "npm run dev:setup:full && concurrently \"npm run dev:auth\" \"npm run dev:gateway\" \"npm run dev:content\"",
    "dev:clean:start": "npm run dev:infra:restart && npm run prisma:generate:all && concurrently \"npm run dev:auth\" \"npm run dev:gateway\" \"npm run dev:content\"",
    "dev:setup:smart": "node setup-dev.js"
  },
  "private": true,
  "prisma": {
    "seed": "npx ts-node -P tsconfig.seed.json ./apps/content-service/prisma/seed.ts"
  },
