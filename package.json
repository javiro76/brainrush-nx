{
  "name": "@brainrush-nx/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev:infra:up": "docker compose -f docker-compose.dev.yml up -d",
    "dev:infra:down": "docker compose -f docker-compose.dev.yml down",
    "dev:infra:logs": "docker compose -f docker-compose.dev.yml logs -f",
    "dev:auth": "npx nx serve auth-service",
    "dev:gateway": "npx nx serve api-gateway",
    "dev:all": "concurrently \"npm run dev:infra:up\" \"npm run dev:auth\" \"npm run dev:gateway\"",
    "prisma:migrate": "npx prisma migrate deploy --schema=./apps/auth-service/prisma/schema.prisma",
    "prisma:studio": "npx prisma studio --schema=./apps/auth-service/prisma/schema.prisma",
    "dev:setup": "npm run dev:infra:up && npm run prisma:migrate",
    "dev:frontend": "npx nx serve brainrush-frontend",
    "dev:all-with-frontend": "concurrently \"npm run dev:infra:up\" \"npm run dev:auth\" \"npm run dev:gateway\" \"npm run dev:frontend\""
  },
  "private": true,
  "dependencies": {
